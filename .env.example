# Core LiveKit API Keys
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret
LIVEKIT_URL=wss://your-livekit-url.livekit.cloud

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key

# Mem0 API Key (for memory management)
MEM0_API_KEY=your_mem0_api_key

# Gmail OAuth Credentials
GMAIL_CLIENT_ID=your_gmail_client_id
GMAIL_CLIENT_SECRET=your_gmail_client_secret
GMAIL_REDIRECT_URI=http://localhost:8000/gmail/callback

# Google Calendar OAuth Credentials (can use same as Gmail)
CALENDAR_CLIENT_ID=your_gmail_client_id
CALENDAR_CLIENT_SECRET=your_gmail_client_secret
CALENDAR_REDIRECT_URI=http://localhost:8000/calendar/callback

# Tavily Search API Key (for web search capabilities)
# Sign up at: https://tavily.com
# Free tier: 1,000 searches/month (no credit card required)
# Pay as you go: $0.008 per credit
# Project plan: $30/month for 4,000 credits
TAVILY_API_KEY=your_tavily_api_key

# Server Configuration
AUTH_SERVER_URL=http://localhost:8000
DATA_DIR=data

# User Preferences
USER_TIMEZONE=America/Los_Angeles
